<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd
          http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.2.xsd">

    <bean id="popServiceImpl" class="org.kuali.student.poc.rules.population.PopulationServiceMapImpl" />
    <bean id="atpServiceImpl" class="org.kuali.student.r2.core.class1.atp.service.impl.AtpServiceMapImpl"/>

    <bean id="gesServiceImpl" class="org.kuali.student.enrollment.class2.ges.service.impl.GesServiceMapImpl">
        <property name="populationService" ref="popServiceImpl"/>
        <property name="atpService" ref="atpServiceImpl"/>
    </bean>

    <bean id="gesDataLoader" class="org.kuali.student.enrollment.class2.ges.service.GesServiceDataLoader" />

    <bean id="popDataLoader" class="org.kuali.student.enrollment.class2.population.PopulationServiceDataLoader" />
</beans>