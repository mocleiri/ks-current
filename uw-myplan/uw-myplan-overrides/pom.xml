<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.kuali.student.myplan</groupId>
    <artifactId>uw-myplan-overrides</artifactId>
    <packaging>jar</packaging>
    <name>Uw myplan overrides</name>
    <description>Project for building MyPlan Reference Wars</description>

    <parent>
        <groupId>org.kuali.student.myplan</groupId>
        <artifactId>uw-myplan</artifactId>
        <version>1.9.0-SNAPSHOT</version>
    </parent>


    <properties>
        <derby.version>10.4.2.0</derby.version>
        <ks.api.version>2.0.1-cm</ks.api.version>
        <ks.core.version>2.0.1-cm</ks.core.version>
        <ks.enroll.api.version>2.0.0-M7</ks.enroll.api.version>
        <ks.enroll.version>1.0.0-M7</ks.enroll.version>
        <ks.lum.version>2.0.1-cm</ks.lum.version>
    </properties>



    <repositories>
        <repository>
            <id>kuali.nexus</id>
            <name>Nexus Repository Manager</name>
            <url>http://nexus.kuali.org/content/groups/public</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>


        <!-- For Restlet dependency -->
        <repository>
            <id>local</id>
            <name>Public online Restlet repository</name>
            <url>http://localhost:8080</url>
        </repository>

    </repositories>

    <dependencies>

        <!-- add this dependency for dynamic loading of spring beans-->
        <dependency>
            <groupId>org.kuali.rice</groupId>
            <artifactId>rice-development-tools</artifactId>
            <version>${kuali.rice.version}</version>
        </dependency>

        <dependency>
            <groupId>org.mortbay.jetty</groupId>
            <artifactId>jetty-util</artifactId>
            <version>6.1.26</version>
        </dependency>

        <!--
            The following dependencies (as well as an exclude in the ks-lum-impl dependency below)
            are a work-around for a commons-beanutils bug (https://issues.apache.org/jira/browse/BEANUTILS-340)
            and should be removed once the bug is resolved or upstream dependencies on beanutils is changed
            to org.kuali.commons:commons-beanutils:1.8.3-kuali.
        -->
        <dependency>
            <groupId>org.kuali.commons</groupId>
            <artifactId>commons-beanutils</artifactId>
            <version>1.8.3-kuali</version>
        </dependency>
        <!-- -->
        <dependency>
            <groupId>commons-digester</groupId>
            <artifactId>commons-digester</artifactId>
            <version>2.1</version>
            <exclusions>
                <exclusion>
                    <groupId>commons-beanutils</groupId>
                    <artifactId>commons-beanutils</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.kuali.rice</groupId>
            <artifactId>rice-core-impl</artifactId>
            <version>${kuali.rice.version}</version>
            <exclusions>
                <exclusion>
                    <groupId>commons-beanutils</groupId>
                    <artifactId>commons-beanutils</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>javax.servlet</groupId>
                    <artifactId>servlet-api</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>javax.servlet.jsp</groupId>
                    <artifactId>jsp-api</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.core</groupId>
            <artifactId>ks-common-impl</artifactId>
            <version>${ks.core.version}</version>
            <exclusions>
                <exclusion>
                    <groupId>commons-beanutils</groupId>
                    <artifactId>commons-beanutils</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>net.sf.jasperreports</groupId>
            <artifactId>jasperreports</artifactId>
            <version>4.0.1</version>
            <exclusions>
                <exclusion>
                    <groupId>commons-beanutils</groupId>
                    <artifactId>commons-beanutils</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <!-- End beanutils work-around -->

        <dependency>
            <groupId>org.springframework</groupId>
            <artifactId>spring-aspects</artifactId>
        </dependency>

        <dependency>
            <groupId>commons-lang</groupId>
            <artifactId>commons-lang</artifactId>
            <version>2.6</version>
        </dependency>

        <dependency>
            <groupId>org.kuali.rice</groupId>
            <artifactId>rice-web</artifactId>
            <version>${kuali.rice.version}</version>
            <type>war</type>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>org.kuali.rice</groupId>
            <artifactId>rice-impl</artifactId>
            <version>${kuali.rice.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.rice</groupId>
            <artifactId>rice-krms-impl</artifactId>
            <version>${kuali.rice.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.rice</groupId>
            <artifactId>rice-kns</artifactId>
            <version>${kuali.rice.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.rice</groupId>
            <artifactId>rice-web</artifactId>
            <version>${kuali.rice.version}</version>
            <type>pom</type>
        </dependency>
        <dependency>
            <groupId>org.kuali.rice</groupId>
            <artifactId>rice-edl-impl</artifactId>
            <version>${kuali.rice.version}</version>
        </dependency>

        <dependency>
            <groupId>org.kuali.student.lum</groupId>
            <artifactId>ks-lum-rice</artifactId>
            <version>${ks.lum.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.api</groupId>
            <artifactId>ks-lum-api</artifactId>
            <version>${ks.api.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.lum</groupId>
            <artifactId>ks-lum-impl</artifactId>
            <version>${ks.lum.version}</version>
            <!--  This is a workaround for the commons-beanutils bug mentioned above and
                should be removed once the issue is resolved. -->
            <exclusions>
                <exclusion>
                    <groupId>commons-beanutils</groupId>
                    <artifactId>commons-beanutils</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.lum</groupId>
            <artifactId>ks-lum-ui</artifactId>
            <version>${ks.lum.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.core</groupId>
            <artifactId>ks-core-impl</artifactId>
            <version>${ks.core.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.core</groupId>
            <artifactId>ks-common-ui</artifactId>
            <version>${ks.core.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.core</groupId>
            <artifactId>ks-sts</artifactId>
            <version>${ks.core.version}</version>
        </dependency>

        <dependency>
            <groupId>org.kuali.student.myplan</groupId>
            <artifactId>myplan-impl</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.myplan</groupId>
            <artifactId>myplan-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.myplan</groupId>
            <artifactId>myplan-ui</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kuali.student.myplan</groupId>
            <artifactId>myplan-web</artifactId>
            <version>${project.version}</version>
            <type>war</type>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>backport-util-concurrent</groupId>
            <artifactId>backport-util-concurrent</artifactId>
        </dependency>
        <dependency>
            <groupId>net.sf.jsr107cache</groupId>
            <artifactId>jsr107cache</artifactId>
        </dependency>

        <dependency>
            <groupId>antlr</groupId>
            <artifactId>antlr</artifactId>
        </dependency>

        <dependency>
            <groupId>org.springframework.security</groupId>
            <artifactId>spring-security-config</artifactId>
            <version>${spring.security.version}</version>
        </dependency>

        <!-- Mutual certificate authentication between UW KS and UW Rice -->
        <dependency>
            <groupId>edu.uw.kuali</groupId>
            <artifactId>uw-commons-security</artifactId>
            <version>1.0.1</version>
        </dependency>


        <dependency>
            <groupId>org.kuali.rice</groupId>
            <artifactId>rice-krad-development-tools</artifactId>
            <version>${kuali.rice.version}</version>
        </dependency>

        <dependency>
            <groupId>org.apache.solr</groupId>
            <artifactId>solr-core</artifactId>
            <version>4.3.0</version>
        </dependency>
    </dependencies>



</project>