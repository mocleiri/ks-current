<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-lazy-init="true">

    <!-- Declaration of the following beans is necessary to disable the KIM UI encryption of the document forms -->
    <bean id="encryptionService" class="org.kuali.rice.core.impl.encryption.NoEncryptionEncryptionServiceImpl"/>
    <bean id="enEncryptionService" class="org.kuali.rice.core.impl.encryption.NoEncryptionEncryptionServiceImpl"/>

</beans>
