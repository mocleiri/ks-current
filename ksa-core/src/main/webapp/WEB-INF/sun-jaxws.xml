<endpoints xmlns="http://java.sun.com/xml/ns/jax-ws/ri/runtime" version="2.0">

    <!--  TransactionService WS Endpoint -->
    <endpoint name="TransactionService"
              implementation="com.sigmasys.kuali.ksa.service.impl.TransactionServiceImpl"
              url-pattern="/transaction.webservice"/>

    <!--  TransactionImportService WS Endpoint -->
    <endpoint name="TransactionImportService"
              implementation="com.sigmasys.kuali.ksa.service.impl.TransactionImportServiceImpl"
              url-pattern="/transactionImport.webservice"/>

    <!--  TransactionExportService WS Endpoint -->
    <endpoint name="TransactionExportService"
              implementation="com.sigmasys.kuali.ksa.service.impl.TransactionExportServiceImpl"
              url-pattern="/transactionExport.webservice"/>

    <!--  LocalizationService WS Endpoint -->
    <endpoint name="LocalizationService"
              implementation="com.sigmasys.kuali.ksa.service.impl.LocalizationWebService"
              url-pattern="/localization.webservice"/>

</endpoints>