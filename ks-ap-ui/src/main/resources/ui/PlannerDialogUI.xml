<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="PlannerDialog-FormView" parent="MyPlanBaseFormView">
		<property name="headerText" value="Plan" />
		<property name="id" value="PlannerDialog-FormView" />
		<property name="footer.render" value="false" />
		<property name="header.render" value="false" />
		<property name="formClass" value="org.kuali.student.ap.plan.PlannerForm" />
		<property name="onSubmitScript" value="ksapPlannerSubmitDialog(e);" />
		<property name="cssClasses">
			<list merge="true">
				<value>myplan-view</value>
			</list>
		</property>
		<property name="items">
			<list>
				<ref bean="planner_add_course_page" />
				<ref bean="planner_edit_term_note_page" />
				<ref bean="planner_course_summary_page" />
				<ref bean="planner_copy_course_page" />
				<ref bean="planner_edit_plan_item_page" />
				<ref bean="planner_copy_plan_item_page" />
				<ref bean="planner_move_plan_item_page" />
				<ref bean="planner_delete_plan_item_page" />
			</list>
		</property>
	</bean>
	
	<bean id="planner_dialog_page" parent="KSAP-PopoverPage" abstract="true">
		<property name="items">
			<list>
				<bean parent="planner_hidden_input" p:propertyName="learningPlanId" />
				<bean parent="planner_hidden_input" p:propertyName="termId" />
				<bean parent="planner_hidden_input" p:propertyName="planItemId" />
				<bean parent="planner_hidden_input" p:propertyName="courseId" />
				<bean parent="planner_hidden_input" p:propertyName="uniqueId" />
			</list>
		</property>
	</bean>

	<bean id="planner_add_course_page" parent="planner_dialog_page">
		<property name="id" value="planner_add_course_page" />
		<property name="innerViewWrapperId" value="planner_add_course_inner" />
		<property name="headerText" value="What would you like to add to @{term.name}?" />
		<property name="items">
			<list>
				<bean parent="planner_hidden_input" p:propertyName="learningPlanId" />
				<bean parent="planner_hidden_input" p:propertyName="termId" />
				<bean parent="planner_hidden_input" p:propertyName="planItemId" />
				<bean parent="planner_hidden_input" p:propertyName="uniqueId" />
				
				<bean parent="planner_course_code_input" p:id="planner_add_course_code"/>
<!-- 				<bean parent="planner_course_credit_input" /> -->
				<bean parent="planner_course_id_input"  p:render="@{courseCd ne null}"/>
				<bean parent="planner_course_note_input" />
			</list>
		</property>
		<property name="footer">
			<bean parent="Uif-FooterBase">
				<property name="items">
					<list>
						<bean parent="Uif-VerticalBoxGroup">
							<property name="items">
								<list>
									<bean parent="Uif-HorizontalBoxGroup">
										<property name="items">
											<list>
												<bean parent="planner_submit_button" p:actionLabel="Add to Plan" />
												<bean parent="planner_cancel_button" />
											</list>
										</property>
									</bean>
									<bean parent="planner_error_placeholder" />
								</list>
							</property>
						</bean>
					</list>
				</property>
			</bean>
		</property>
	</bean>

  <bean id="planner_edit_term_note_page" parent="planner_dialog_page">
		<property name="id" value="planner_edit_term_note_page" />
		<property name="innerViewWrapperId" value="planner_edit_term_note_inner" />
		<property name="headerText" value="Edit @{term.name} term note" />
    	<property name="items">
	    	<list merge="true">
				<bean parent="planner_term_note_input" />
			</list>
		</property>
		<property name="footer">
			<bean parent="Uif-FooterBase">
				<property name="items">
					<list>
						<bean parent="Uif-VerticalBoxGroup">
							<property name="items">
								<list>
									<bean parent="Uif-HorizontalBoxGroup">
										<property name="items">
											<list>
												<bean parent="planner_submit_button" p:actionLabel="Save" />
												<bean parent="planner_cancel_button" />
											</list>
										</property>
									</bean>
									<bean parent="planner_error_placeholder" />
								</list>
							</property>
						</bean>
					</list>
				</property>
			</bean>
		</property>
	</bean>

  <bean id="planner_course_summary_page" parent="KSAP-PopoverPage">
		<property name="id" value="planner_course_summary_page" />
		<property name="innerViewWrapperId" value="planner_course_summary_inner" />
		<property name="headerText" value="Course Summary" />
	    <property name="items">
    		<list merge="true">
				<ref bean="planner_course_summary_link" />
				<ref bean="planner_course_details_summary_info"/>
			</list>
		</property>
		<property name="footer">
			<bean parent="Uif-FooterBase">
				<property name="items">
					<list>
						<bean parent="planner_course_summary_link">
							<property name="linkText" value="View more details"/>
							<property name="title" value="View more details about @{courseSummaryDetails.code}"/>
						</bean>
					</list>
				</property>
			</bean>
		</property>
	</bean>

	<bean id="planner_course_summary_link" parent="Uif-Link" p:target="_blank">
		<property name="cssClasses">
			<list merge="true">
				<value>fl-text-bold</value>
				<value>myplan-text-wrap</value>
				<value>ks-Popover-bodyLabel</value>
			</list>
		</property>
		<property name="linkText" value="@{course.code} @{course.courseTitle} (@{creditString})"/>
		<property name="title" value="@{course.code} @{course.courseTitle} (@{creditString})"/>
		<property name="href" value="inquiry?methodToCall=start&amp;viewId=CourseDetails-InquiryView&amp;courseId=@{courseId}"/>
	</bean>

	<bean id="planner_course_details_summary_info" parent="Uif-VerticalBoxGroup">
		<property name="validationMessages.render" value="false"/>
		<property name="items">
			<list>
				<ref bean="planner_course_summary_details_data" />
				<bean parent="planner_course_note_input" p:render="@{planItemId ne null}">
					<property name="control.disabled" value="true" />
					<property name="control.watermarkText" value="" />
				</bean>
			</list>
		</property>
	</bean>

	<bean id="planner_course_summary_details_data" parent="Uif-VerticalBoxGroup">
		<property name="validationMessages.render" value="false"/>
		<property name="cssClasses">
			<list merge="true">
				<value>myplan-course-details</value>
				<value>ks-Popover-item</value>
				<value>ks-Popover-item--divideBefore</value>
				<value>ks-Popover-item--divideAfter</value>
				<value>ks-Popover-item--secondary</value>
			</list>
		</property>
		<property name="items">
			<list>
				<!-- Scheduled Terms -->
				<bean parent="Uif-DataField" p:label="Scheduled for" p:propertyName="courseSummaryDetails.scheduledTermsNames"
						p:fieldLabel.cssClasses="fl-text-gray myplan-text-normal ks-Popover-detailLabel" p:labelPlacement="LEFT">
					<property name="cssClasses">
						<list merge="true">
							<value>myplan-data-list</value>
							<value>ks-Popover-detail</value>
						</list>
					</property>
					<property name="render" value="@{courseSummaryDetails.scheduledTerms.size() gt 0}"/>
				</bean>
				<bean parent="Uif-MessageField" p:label="Scheduled for"
						p:fieldLabel.cssClasses="fl-text-gray myplan-text-normal ks-Popover-detailLabel"
						p:render="@{courseSummaryDetails.scheduledTerms.size() eq 0}">
					<property name="message">
						<bean parent="Uif-Message" p:messageText="Not currently scheduled"/>
					</property>
				</bean>
				<!-- Last Offered -->
				<bean parent="Uif-DataField" p:label="Last Offered" p:propertyName="courseSummaryDetails.lastOffered" p:labelPlacement="LEFT">
					<property name="fieldLabel.cssClasses" value="fl-text-gray myplan-text-normal ks-Popover-detailLabel"/>
					<property name="render" value="@{courseSummaryDetails.lastOffered ne null and courseSummaryDetails.scheduledTerms.size() eq 0}"/>
				</bean>
				<!-- Projected Terms -->
				<bean parent="Uif-DataField" p:label="Projected for" p:propertyName="courseSummaryDetails.termsOffered"
						p:labelPlacement="LEFT" p:escapeHtmlInPropertyValue="false">
					<property name="fieldLabel.cssClasses" value="fl-text-gray myplan-text-normal ks-Popover-detailLabel"/>
					<property name="propertyEditor" ref="terms_formatter"/>
					<property name="cssClasses">
						<list merge="true">
							<value>myplan-data-list</value>
							<value>ks-Popover-detail</value>
						</list>
					</property>
					<property name="render" value="@{courseSummaryDetails.scheduledTerms.size() eq 0}"/>
				</bean>
				<!-- Requisites -->
				<bean parent="Uif-DataField" p:label="Course Requisites"
						p:propertyName="courseSummaryDetails.requisites" p:labelPlacement="LEFT" p:escapeHtmlInPropertyValue="false">
					<property name="fieldLabel.cssClasses" value="fl-text-gray myplan-text-normal ks-Popover-detailLabel"/>
					<property name="propertyEditor" ref="data_list_formatter"/>
					<property name="cssClasses">
						<list merge="true">
							<value>myplan-data-list</value>
							<value>myplan-multiline-ellipsis</value>
							<value>ks-Popover-detail</value>
						</list>
					</property>
				</bean>
				<bean parent="Uif-Disclosure-StackedCollectionSection" p:render="@{activityOptions ne null and !activityOptions.empty}"
						p:additionalCssClasses="ksap-planner-activity@{registrationCode ne null?'':'-options'}" p:renderAddLine="false"
						p:renderSaveLineActions="false" p:renderLineActions="false" p:includeLineSelectionField="false">
					<property name="propertyName" value="activityOptions" />
					<property name="headerText" value="@{term.name} Classes" />
					<property name="header.render" value="@{registrationCode eq null}" />
					<property name="disclosure.defaultOpen" value="@{registrationCode ne null}" />
					<property name="collectionObjectClass" value="org.kuali.student.ap.sb.infc.ActivityOption" />
				    <property name="layoutManager">
						<bean parent="Uif-StackedCollectionLayout-WithBoxItems">
							<property name="lineGroupPrototype">
								<bean parent="Uif-CollectionVerticalBoxItem" p:footer.render="false"  p:additionalCssClasses="ksap-planner-activity-row"/>
							</property>
						</bean>
					</property>
					<property name="items">
						<list>
							<bean parent="Uif-VerticalFieldGroup" p:additionalCssClasses="ksap-planner-activity-row">
								<property name="items">
									<list>
										<bean parent="Uif-VerticalBoxSection" p:additionalCssClasses="ksap-planner-activity-info">
											<property name="validationMessages.render" value="false" />
											<property name="headerText" value="@{#line.registrationCode} @{#line.activityTypeDescription} - @{#line.closed?'Closed ':'Open '}@{#line.openSeats}/@{#line.totalSeats}" />
											<property name="items">
												<list>
													<bean parent="Uif-HorizontalBoxGroup">
														<property name="validationMessages.render" value="false" />
														<property name="items">
															<list>
																<bean parent="Uif-Message" p:messageText="@{#line.academicSessionDescr}" p:render="@{!#line.enrollmentGroup || #line.secondaryOptions.empty}"
																		p:additionalCssClasses="ksap-planner-activity-session" />
															</list>
														</property>
													</bean>
													<bean parent="Uif-Message" p:messageText="Permission Required" p:render="@{#line.requiresPermission}" />
													<bean parent="Uif-StackedCollectionGroup" p:additionalCssClasses="ksap-planner-activity-classtimes"
															p:renderAddLine="false" p:renderSaveLineActions="false" p:renderLineActions="false"
															p:render="@{!#line.enrollmentGroup || #line.secondaryOptions.empty}">
														<property name="propertyName" value="classMeetingTimes" />
														<property name="collectionObjectClass" value="org.kuali.student.ap.sb.infc.ClassMeetingTime" />
														<property name="layoutManager">
															<bean parent="Uif-StackedCollectionLayout-WithBoxItems">
																<property name="lineGroupPrototype">
																	<bean parent="Uif-CollectionVerticalBoxItem" />
																</property>
															</bean>
														</property>
														<property name="items">
															<list>
																<bean parent="Uif-HorizontalFieldGroup">
																	<property name="items">
																		<list>
																			<bean parent="Uif-Message" p:messageText="@{!#line.allDay?#line.daysAndTimes:'Meeting time to be arranged'} &amp;bull; @{#line.instructorName}@{#line.instructorName ne null?' &amp;bull; ':''}@{#line.arranged?#line.location:'ARR'}" p:additionalCssClasses="ksap-sb-cart-meetingtime" />
																		</list>
																	</property>
																</bean>
															</list>
														</property>
													</bean>
													<bean parent="Uif-StackedCollectionGroup" p:renderAddLine="false" p:renderSaveLineActions="false"
															p:renderLineActions="false" p:render="@{#line.notes ne null and !#line.notes.empty}"
															p:additionalCssClasses="ksap-planner-activity-classnotes">
														<property name="propertyName" value="notes" />
														<property name="collectionObjectClass" value="java.lang.String" />
														<property name="layoutManager">
															<bean parent="Uif-StackedCollectionLayout-WithBoxItems">
																<property name="lineGroupPrototype">
																	<bean parent="Uif-CollectionVerticalBoxItem" p:additionalCssClasses="ksap-sb-activity-classnotes-row" />
																</property>
															</bean>
														</property>
														<property name="items">
															<list>
																<bean parent="Uif-MessageField" p:messageText="@{#line}" />
															</list>
														</property>
													</bean>
												</list>
											</property>
										</bean>
									</list>
								</property>
							</bean>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<bean id="planner_edit_plan_item_page" parent="planner_dialog_page">
		<property name="id" value="planner_edit_plan_item_page" />
		<property name="innerViewWrapperId" value="planner_edit_plan_item_inner" />
		<property name="headerText" value="Edit @{course.code} @{termId eq null ? 'Bookmark' : 'from the '+term.name+' plan'}" />
