	<bean id="planner_hidden_input" parent="Uif-InputField" abstract="true">
		<property name="hidden" value="true" />
		<property name="control">
			<bean parent="Uif-HiddenControl" />
		</property>
	</bean>

	<bean id="planner_dialog_input" parent="Uif-InputField" abstract="true">
		<property name="labelPlacement" value="TOP" />
		<property name="fieldLabel.render" value="true" />
		<property name="fieldLabel.cssClasses">
			<list merge="true">
				<value>myplan-text-normal</value>
			</list>
		</property>
	</bean>

	<bean id="planner_term_input" parent="planner_dialog_input">
		<property name="propertyName" value="targetTermId" />
		<property name="label" value="Term &amp; Year" />
		<property name="instructionalText" value="Note: This course MAY NOT be offered in your selected term. Check back after the schedule of classes has been released." />
		<property name="control">
			<bean parent="Uif-DropdownControl"/>
		</property>
		<property name="optionsFinder">
			<bean class="org.kuali.student.myplan.plan.util.TermsListBuilder"/>
		</property>
	</bean>

	<bean id="planner_course_code_input" parent="planner_dialog_input">
		<property name="propertyName" value="courseCd" />
		<property name="label" value="Enter subject and catalog number" />
		<property name="control">
			<bean parent="Uif-TextControl" p:additionalCssClasses="ksap-planner-courseCode">
				<property name="onDocumentReadyScript" value="jQuery('#courseCode_control').focus();" />
				<property name="watermarkText" value="e.g. BL MATH-M 119" />
				<property name="maxLength" value="50" />
				 <property name="size" value ="25"/>
			</bean>
		</property>
	</bean>

